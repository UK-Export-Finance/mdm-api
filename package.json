{
  "name": "mdm-api",
  "version": "1.16.0",
  "private": true,
  "description": "Master Data Management API",
  "license": "MIT",
  "author": "UKEF",
  "scripts": {
    "api-test": "jest --selectProjects=API",
    "build": "nest build -p tsconfig.build.json",
    "housekeeping": "npm update --save --legacy-peer-deps && npm i --legacy-peer-deps && npm audit --fix && npm run spellcheck",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "spellcheck": "cspell lint --gitignore --no-must-find-files --unique --no-progress --show-suggestions --color '**/*'",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --tsc --watch",
    "start:prod": "node dist/src/main",
    "unit-test": "jest --selectProjects=Unit"
  },
  "lint-staged": {
    "**/package.json": "sort-package-json",
    "**/*.js": "eslint --fix",
    "**/*.ts": "eslint --fix",
    "**/*": [
      "cspell lint --gitignore --no-must-find-files --unique --no-progress --show-suggestions --color"
    ]
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express-basic-auth": "^1.2.1",
    "lodash": "^4.17.21",
    "mssql": "^10.0.1",
    "nestjs-pino": "^3.5.0",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^3.0.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@tsconfig/node20": "^20.1.2",
    "@types/chance": "^1.1.4",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.6",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "chance": "^1.1.11",
    "cspell": "^7.3.8",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-when": "^3.6.0",
    "lint-staged": "^14.0.1",
    "nock": "^13.3.4",
    "prettier": "^3.0.3",
    "sort-package-json": "^2.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9.8.0"
  }
}
